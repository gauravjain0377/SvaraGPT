{
    "version": 2,
    "name": "svaragpt-backend",
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node",
            "config": {
                "maxDuration": 30
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "functions": {
        "server.js": {
            "maxDuration": 30,
            "memory": 1024
        }
    }
}